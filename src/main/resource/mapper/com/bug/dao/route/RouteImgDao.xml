<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.bug.dao.route.RouteImgDao">
    <!--List<RouteImg> findByRid(int rid);-->
    <select id="findByRid" resultType="com.bug.domain.route.RouteImg">
        select *
        from tab_route_img
        where rid = #{id}
    </select>

</mapper>